{
    "env": {
        "node": true,
        "es2020": true
    },
    "extends": [
        "prettier",
        "airbnb-base",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2022
    },
    "rules": {
        "prettier/prettier": ["error"],
        "linebreak-style": "off",
        "max-classes-per-file": "off",
        "no-console": "off",
        "no-shadow":"off",
        "lines-between-class-members": "off",
        "prefer-destructuring": "off",
        "no-continue": "off",
        "no-restricted-syntax": "off",
        "prefer-const": "off"
    }
}
